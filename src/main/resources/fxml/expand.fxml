<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" fitToWidth="true" xmlns="http://javafx.com/javafx/9.0.1"
            fx:controller="ogamebot.gui.Expand">
    <content>
        <VBox>
            <children>
                <HBox alignment="CENTER_LEFT" spacing="5.0">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Bereit am"/>
                        <Text fx:id="availableAt" layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0"/>
                        <Text layoutX="33.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ressourcen:"/>
                        <Text fx:id="availableMetal" layoutX="56.0" layoutY="23.0" strokeType="OUTSIDE"
                              strokeWidth="0.0"/>
                        <Text fx:id="availableCrystal" layoutX="79.0" layoutY="23.0" strokeType="OUTSIDE"
                              strokeWidth="0.0"/>
                        <Text fx:id="availableDeut" layoutX="102.0" layoutY="23.0" strokeType="OUTSIDE"
                              strokeWidth="0.0"/>
                        <Region HBox.hgrow="ALWAYS"/>
                        <Button fx:id="queueButton" maxWidth="-Infinity" mnemonicParsing="false"
                                onAction="#executeQueue" prefWidth="80.0"/>
                    </children>
                    <VBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                    </VBox.margin>
                </HBox>
                <TableView fx:id="queueTable" prefHeight="250.0"/>
                <GridPane fx:id="grid">
                </GridPane>
            </children>
        </VBox>
    </content>
</ScrollPane>
